    "jsdom": "27.3.0",
    "next": "16.0.10",
    "next-intl": "4.6.0",
    "primeicons": "7.0.0",
    "primereact": "10.9.7",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.68.0",
    "uuid": "13.0.0",
