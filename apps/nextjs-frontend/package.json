    "@tanstack/react-query-devtools": "5.83.0",
    "dotenv": "17.2.0",
    "jsdom": "27.0.0",
    "next": "15.4.2",
    "next-intl": "4.3.4",
    "primeicons": "7.0.0",
    "primereact": "10.9.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.59.0",
    "uuid": "13.0.0",
    "zod": "4.1.12",
