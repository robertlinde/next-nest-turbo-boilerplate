    "@tanstack/react-query-devtools": "5.91.3",
    "dotenv": "17.3.1",
    "jsdom": "28.1.0",
    "next": "16.1.6",
    "next-intl": "4.8.3",
    "primeicons": "7.0.0",
    "primereact": "10.9.7",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "7.71.1",
    "uuid": "13.0.0",
    "zod": "4.3.6",
    "zustand": "5.0.11"
